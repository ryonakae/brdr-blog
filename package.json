{
  "scripts": {
    "clean": "rimraf theme/*",
    "server": "browser-sync start -c 'bs-config.js'",
    "copy:public": "cpx 'public/**/{*,.*}' theme -u",
    "watch:public": "chokidar 'public' -c 'yarn copy:public'",
    "watch:webpack": "webpack -w",
    "build:webpack": "NODE_ENV=production webpack",
    "format:js": "prettier-eslint 'src/!(library)/*.{js,vue}' --write",
    "lint:css": "stylelint 'src/**/*.{css,vue}'",
    "copy": "run-p copy:*",
    "watch": "run-p watch:* server",
    "start": "run-s clean copy watch",
    "build": "run-s clean copy build:*",
    "format": "run-s format:*",
    "lint": "run-s lint:*"
  },
  "dependencies": {
